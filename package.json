{
  "name": "woocommerce-radio-buttons",
  "version": "0.4.0",
  "description": "Woocommerce Radio Buttons",
  "private": true,
  "scripts": {
		"start": "wp-scripts start assets/css/woo-radio-variations.js assets/js/frontend/add-to-cart-variation-radio.js --output-path=build/",
		"start:user-js": "wp-scripts start assets/js/frontend/add-to-cart-variation-radio.js --output-path=build/",
		"start:user-css": "wp-scripts start assets/css/woo-radio-variations.js --output-path=build/",
    "build": "wp-scripts build assets/css/woo-radio-variations.js assets/js/frontend/add-to-cart-variation-radio.js --output-path=build/",
		"build:user-js": "wp-scripts build assets/js/frontend/add-to-cart-variation-radio.js --output-path=build/",
		"build:user-css": "wp-scripts build assets/css/woo-radio-variations.js --output-path=build/",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style ./assets",
    "lint:js": "wp-scripts lint-js ./assets",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
		"prepare": "husky install",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helgatheviking/woocommerce-radio-buttons.git"
  },
  "author": "helgatheviking",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/helgatheviking/woocommerce-radio-buttons/issues"
  },
  "homepage": "https://github.com/helgatheviking/woocommerce-radio-buttons#readme",
  "devDependencies": {
    "@wordpress/scripts": "^21.0.2",
    "husky": "^7.0.4"
  }
}
